<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import sh.shinterface.view.tab.BoardPane?>
<fx:root type="sh.shinterface.view.tab.BoardPane" xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml">
    <VBox>
        <ImageView fx:id="liberalBoard" preserveRatio="true" fitHeight="150">
            <Image url="@/sh/shinterface/images/boards/liberal/0.png"/>
        </ImageView>
        <ImageView fx:id="fascistBoard" preserveRatio="true" fitHeight="150"/>
    </VBox>
    <HBox>
        <Button fx:id="gameButton" text="End Game" onAction="#endGame"/>
    </HBox>
</fx:root>
